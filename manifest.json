{
  "manifest_version": 2,

  "name": "Blocker",
  "description": "This extension will make you productive",
  "version": "1.0",
    "content_scripts": [
    {
        "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
    "<all_urls>",
      "webRequest"
      
   ]
}