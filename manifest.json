{
  "manifest_version": 2,

  "name": "GTmetrix Analyzer Plugin",
  "description": "This extension will analyze a page using GTmetrix",
  "version": "1.0",
    "content_scripts": [
    {

      "js": ["popup.js"]
    }],

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
    "<all_urls>",
      "http://brillinat-wolf.tech"
   ]
}